#!/usr/bin/bash
username='tomcat';
echo -e "enter the username for the server(Default:tomcat)----->"
read username;
echo ${username};
echo -e "enter the password must be included(leave empty to configure manually)--->"
read password;
if [[ ${#password} -eq 0 ]] 
then
    echo -e "zero";
else
    echo -e "not zero";
fi

